pages = {1332-1339},
keywords = {investment, invertors, demand side management, power grids, power distribution control, photovoltaic power systems, power generation control, power distribution economics, power generation economics, battery storage plants, PV hosting capacity, photovoltaic generation hosting capacity, low voltage distribution networks, connection points, PV penetration, smart inverter control strategies, battery storage systems, actual solar radiation data, network HC, network investments, LV distribution networks, residential demand profiles},
